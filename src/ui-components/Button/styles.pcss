.emails-button {
  background: var(--blue-color);
  border-radius: 6px;
  font-size: 14px;
  line-height: 24px;
  color: white;
  padding: 8px 16px;
  margin: 8px;
  outline: none;
  transition: background-color var(--transition-time);
  cursor: pointer;
  border: none;

  &:hover {
    background: var(--blue-color-hover);
  }
}
